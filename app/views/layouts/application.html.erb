<!DOCTYPE html>
<html>
<head>
  <title>Prgmgnt</title>
  <%= stylesheet_link_tag "scaffold"%>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="banner">
  <%= image_tag("logo.png")%>
  <%= @page_title||"COUL COR." %>
</div>
<div id="columns">
    <div id="side">
      <a href="#">Home<a><br />
      <a href="#">Contract<a><br />
      <a href="#">About<a><br />
        <%= link_to "Users", users_path%><br/>
        
      <% if session[:user_id]%>
        <br />
        <%= link_to "Projects", projects_path%><br />
        <%= link_to "Users", users_path%><br/>
        <br />
        <%= button_to "Logout", logout_path, :method => :delete %><br/>
      <% end %>
    </div>
    <div id ="main">
      <%= yield %>
    </div>
  </div>
</body>
</html>
